SELECT * FROM MDL_CLM_ONE_YR;

SELECT *, CASE 
          WHEN RANDOM()> 0.5 THEN 1 
          ELSE 2
          END  AS CHUNK_NBR
FROM MDL_CLM_ONE_YR;

-- CREATE A TABLE WHICH CHUNK

CREATE OR REPLACE TABLE MDL_CLM_ONE_YR_CHUNK AS 
SELECT *, CASE 
          WHEN RANDOM()> 0.5 THEN 1 
          ELSE 2
          END  AS CHUNK_NBR
FROM MDL_CLM_ONE_YR;


